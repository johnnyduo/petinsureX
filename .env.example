# Environment Variables for PetInsureX
# Copy this file to .env and fill in your actual values

# SEA-LION AI API Configuration
# Get your API key from https://playground.sea-lion.ai/
VITE_SEA_LION_API_KEY=your_sea_lion_api_key_here
VITE_SEA_LION_API_BASE_URL=https://api.sea-lion.ai/v1

# Development Configuration
VITE_API_BASE_URL=http://localhost:3000/api
VITE_ENABLE_MOCK_DATA=true

# Application Configuration
VITE_APP_NAME=PetInsureX
VITE_APP_VERSION=1.0.0